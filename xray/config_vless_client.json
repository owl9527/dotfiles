{
    "inbounds": [
        {
            "listen": "127.0.0.1",
            "port": 0,
            "protocol": "socks",
            "settings": {
                "udp": true
            }
        }
    ],
    "outbounds": [
        {
            "tag": "proxy",
            "protocol": "vless",
            "settings": {
                "vnext": [
                    {
                        "address": "",
                        "port": 0,
                        "users": [
                            {
                                "id": "",
                                "encryption": "none"
                            }
                        ]
                    }
                ]
            },
            "streamSettings": {
                "security": "reality",
                "realitySettings": {
                    "fingerprint": "",
                    "serverName": "",
                    "password": "",
                    "shortId": ""
                }
            }
        },
        {
            "tag": "direct",
            "protocol": "freedom"
        }
    ],
    "routing": {
        "rules": [
            {
                "ip": [
                    "geoip:private",
                    "geoip:cn"
                ],
                "outboundTag": "direct"
            },
            {
                "domain": [
                    "geosite:google",
                    "geosite:apple",
                    "geosite:paypal",
                    "geosite:microsoft"
                ],
                "outboundTag": "proxy"
            },
            {
                "domain": [
                    "geosite:cn",
                    "geosite:private"
                ],
                "outboundTag": "direct"
            }
        ]
    }
}
