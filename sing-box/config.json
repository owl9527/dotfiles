{
   "inbounds": [
      {
         "type": "mixed",
         "set_system_proxy": true,
         "listen_port": 10000
      }
   ],
   "outbounds": [
      {
         "tag": "proxy",
         "type": "trojan",
         "server": "",
         "server_port": 0,
         "password": "",
         "tls": {
            "enabled": true,
            "server_name": "",
            "insecure": true
         }
      },
      {
         "tag": "direct",
         "type": "direct"
      }
   ],
   "dns": {
      "strategy": "ipv4_only",
      "servers": [
         {
            "address": "https://dns.google/dns-query",
            "address_resolver": "dns_ali",
            "address_strategy": "ipv4_only"
         },
         {
            "tag": "dns_ali",
            "address": "223.5.5.5",
            "detour": "direct"
         }
      ],
      "rules": [
         {
            "outbound": "any",
            "server": "dns_ali"
         }
      ]
   },
   "route": {
      "rules": [
         {
            "ip_is_private": true,
            "outbound": "direct"
         },
         {
            "rule_set": [
               "geosite-apple",
               "geosite-google",
               "geosite-microsoft"
            ],
            "outbound": "proxy"
         },
         {
            "rule_set": "geosite-cn",
            "outbound": "direct"
         },
         {
            "action": "resolve"
         },
         {
            "rule_set": "geoip-cn",
            "outbound": "direct"
         }
      ],
      "rule_set": [
         {
            "tag": "geosite-apple",
            "type": "remote",
            "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-apple.srs"
         },
         {
            "tag": "geosite-google",
            "type": "remote",
            "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-google.srs"
         },
         {
            "tag": "geosite-microsoft",
            "type": "remote",
            "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-microsoft.srs"
         },
         {
            "tag": "geosite-cn",
            "type": "remote",
            "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-cn.srs"
         },
         {
            "tag": "geoip-cn",
            "type": "remote",
            "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs"
         }
      ]
   }
}
